<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:colorBackground">@color/backgroundColorX</item>
    </style>

    <style name="CustomTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimaryX</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkX</item>
        <item name="colorAccent">@color/colorAccentX</item>
        <item name="android:colorBackground">@color/colorPrimary</item>

    </style>

</resources>
